import java.lang.*;
import java.util.*;

public class Main{
     public static void main(String[]args){
         Scanner sc = new Scanner(System.in);
         int N = sc.nextInt();
         int M = sc.nextInt();
        
        for(int i=1;i<=M;i++){
            int operation = sc.nextInt();
            int Amount = sc.nextInt();
            
            
           if(operation==1){
                N+=Amount;
                System.out.println(N);
            }
            else if (operation==2){
                N-=Amount;
                if(N>0){
                System.out.println(N);}
                else System.out.println("Insufficient funds");}
 
        }
  
     }   
    
}
